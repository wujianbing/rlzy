html,
body {
	width: 100%;
    font-size: 62.5%;
	background: #f5f6f6;
}



* {
	padding: 0;
	margin: 0;
}
.bar{
	    border-bottom: 1px solid #ECE8E9;
	    box-shadow: 0px 5px 30px #e0e0e0;
}
.titleTop {
	width: 100%;
	background: #fff;
}

.rowTop {
	width: 69%;
	background: #fff;
	margin: 0 auto;
	color: #666;
	white-space: nowrap;
	font-size: 16px;
}

.rowBar {
	width: 100%;
	padding: 0 10%;
	height: 60px;
	margin: 0;
	color: #666;
	/*box-shadow: 0px 23px 20px -20px #ddd;*/
	background: #fff;
}

.humanbox {
	width: 100%;
	padding: 0;
	margin: 0;
}

.title {
	height: 80px;
	color: #666;
}

.title h3 {
	font-weight: bolder;
	color: #666;
	font-size: 2.8rem;
}

.title h6 {
	color: #666;
}

.searchBar {
	height: 8rem;
	display: flex;
	align-items: center;
}

.input-group {
	border: none;
	background: #fff;
	padding: 0;
	margin: 0;
}

.input-group .searchBarLeft {
	border: none;
	font-size: 1.5rem;
	background: #f5f6f6;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
	outline: none;
}

.input-group .searchBarRight {
	padding: 0;
	margin: 0;
}

input.form-control {
	outline: none;
	box-shadow: none;
}

.form-control:focus {
	webkit-box-shadow: none;
	box-shadow: none;
}

.input-group .searchBarRight button.btn.btn-default {
	top: 0;
	border: none;
	padding: 7px 12px;
	background: #f5f6f6;
	border-top-right-radius: 15px;
	border-bottom-right-radius: 15px;
	outline: none;
	z-index: 0;
	margin-left: 0px;
}

button.btn.btn-default.glyphicon.glyphicon-search {
	color: #358acb;
	font-weight: bolder;
}

.login {
	color: #666;
	display: inline-block;
	width: 50%;
	text-align: right;
	padding-right: 25%;
}

.registered {
	color: #666;
	display: inline-block;
	width: 50%;
}

ul.nav.nav-tabs {
	width: 100%;
	height: 6rem;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-bottom: 0;
}

ul.nav.nav-tabs li {
	margin-bottom: 0;
	overflow: hidden;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a {
	display: inline-block;
	color: #666;
	font-size: 18px;
	white-space: inherit;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a:hover {
	cursor: pointer;
	text-align: none;
	color: #4b96cf;
	font-weight: bolder;
	background: none;
	border: none;
}

ul.nav.nav-tabs li a:visited,
a:active,
a:link {
	cursor: pointer;
	text-align: none;
	background: none;
}

.banner {
	width: 100%;
	height: 18rem;
	padding:1rem 0;
	background: #fff;
	background: url(../img/bannerFind.png) no-repeat;
	background-size: cover;
}
.infopname1{
	width: 100%;
	display: flex;
	height: 3rem;
	line-height: 3rem;
	font-size: 14px;
	color: #666666;
	justify-content: space-between;
}
.lastname{
	margin-bottom: 20px;
}
.infopname1 .sitename{
	
	
	
}

.rightman{
	float: right;
	font-size: 14px;
}

.webLink {
	width: 80%;
	padding: 25px 0;
	margin: 0 auto;
	display: flex;
	justify-content: space-between;
	background: #f5f6f6;
}
h5 {
	width: 100%;
	margin: 0;
	text-align: center;
	padding: 0;
	font-size: 24px;
	font-weight: bold;
	height: 4rem;
	line-height: 4rem;
	color:#4b96cf;
}
.processReg {
	width: 100%;
	margin: 0;
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 0;
	font-size: 30px;
	font-weight: bold;
	height: 8rem;
	
	color:#999999;
	background: #F2DEDE;
}
.processReg span {
	display: inline-block;
}
.ringnum{
	width: 50px;
	height: 50px;
	text-align: center;
	line-height: 50px;
	border:1px solid #cccc;
	border-radius: 50%;
}
.activeRing{
	background: #4b96cf;
	color:#fff;
	border: 1px solid #4b96cf;
}
.lineSpan{
	width: 13.3rem;
	border: 1px solid #cccccc;
	margin: 0 20px;
}
.yuqbottom {
	width: 68%;
	margin: 40px auto;
	min-height:65%;
	background: #fff;
	display: flex;
	align-items: center;
	justify-content: center;
}
.asp{
	width: 48rem;
    height: 35rem;
    background: #FF6501;
}
.yuqbottom:after{
    content: "";
    height: 0;
    clear: both;
    overflow: hidden;
    display: block;
    visibility: hidden;
}
.webLinkWork {
	margin-top: 2rem;
	width: 100%;
	height: 7rem;
	background: #fff;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 14px;
	color: #666666;
}

.webLinkWork a {
	cursor: pointer;
	text-align: none;
	font-size: 14px;
	color: #666666;
}

.login {
	cursor: pointer;
	text-align: none;
}

.registered {
	cursor: pointer;
	text-align: none;
}

.banquan {
	width: 100%;
	padding: 5px 0;
	text-align: center;
	color: #666666;
	background: #cccccc;
	font-size: 12px;
}

.btn{
	padding: 5px 20px;
	color: #fff;
	background:#4b96cf;
	font-size: 14px;
	
}