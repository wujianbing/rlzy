.select-container {
	position: relative;
}

.select-container input {
	width: 100px;
	outline: none;
	border: 1px #E7E7E7 solid;
	padding: 7px 11px;
	border-radius: 3px;
	color: #666666;
	cursor: pointer;
}

.select-container:hover input {
	border: 1px solid #b0b0b0;
}

.select-container span {
	position: absolute;
	top: 10px;
	right: 6px;
	pointer-events: none;
	color: #666666;
	font-size: 14px;
}

.select-container ul {
	display: none;
	position: absolute;
	top: 31px;
	left: 0px;
	padding: 0;
	z-index: 1;
	border: 1px solid #ddd;
	width: 98%;
	background: #ffffff;
}

.select-container li a {
	cursor: pointer;
