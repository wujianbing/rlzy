ul.nav.nav-tabs {
	width: 100%;
	height: 6rem;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-bottom: 0;
}

ul.nav.nav-tabs li {
	margin-bottom: 0;
	overflow: hidden;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a {
	display: inline-block;
	color: #666;
	font-size: 18px;
	white-space: inherit;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a:hover {
	cursor: pointer;
	text-align: none;
	color: #4b96cf;
	font-weight: bolder;
	background: none;
	border: none;
}

ul.nav.nav-tabs li a:visited,
a:active,
a:link {
	cursor: pointer;
	text-align: none;
	background: none;
}

