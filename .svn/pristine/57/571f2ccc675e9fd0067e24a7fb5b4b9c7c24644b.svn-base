.input-group {
	border: none;
	background: #fff;
	padding: 0;
	margin: 0;
}

.input-group .searchBarLeft {
	border: none;
	font-size: 1.5rem;
	background: #f5f6f6;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
	outline: none;
}

.input-group .searchBarRight {
	padding: 0;
	margin: 0;
}

input.form-control {
	outline: none;
	box-shadow: none;
}

.form-control:focus {
	webkit-box-shadow: none;
	box-shadow: none;
}

.input-group .searchBarRight button.btn.btn-default {
	top: 0;
	border: none;
	padding: 7px 12px;
	background: #f5f6f6;
	border-top-right-radius: 15px;
	border-bottom-right-radius: 15px;
	outline: none;
	z-index: 0;
	margin-left: 0px;
}

button.btn.btn-default.glyphicon.glyphicon-search {
	color: #358acb;
	font-weight: bolder;
}

