html,
body {
	width: 100%;
    font-size: 62.5%;
	background: #f5f6f6;
}
* {
	padding: 0;
	margin: 0;
}

.titleTop{
	padding: 20px 0;
}
.rowTop {
	width: 1200px;
	margin: 0 auto;
	color: #666;
	white-space: nowrap;
	font-size: 16px;
	display: flex;
}
.bar{
	border-bottom: 1px solid #ECE8E9;
	box-shadow: 0px 5px 30px #e0e0e0;
	background: #fff;
}
.rowBar {
	width: 1200px;
	margin: 0;
	color: #666;
	margin: 0 auto;
}
.humanbox {
	width: 100%;
	padding: 0;
	margin: 0;
}

.title {
	color: #666;
	flex: 1;
}

.title h3 {
	font-weight: bolder;
	color: #666;
	font-size: 2.8rem;
}

.title h6 {
	color: #666;
}

.searchBar {
	display: flex;
	align-items: center;
	flex: 1;
	justify-content: flex-end;
	align-items: center;
	padding: 0 12px;
}

.input-group {
	border: none;
	padding: 0;
	margin: 0;
}

.input-group .searchBarLeft {
	border: none;
	font-size: 1.5rem;
	background: #f5f6f6;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
	outline: none;
}
.input-key-v{
	position: relative;
}
.input-key{
	border-radius: 19px;
    font-size: 13px;
    padding: 0 10px 0 15px;
    width: 220px;
    background: #f1f1f1;
}
.search-se{
	position: absolute;
	right: 8px;
	top: 7px;
	font-size: 23px;
	color: #20a1e0;
	cursor: pointer;
}
.searchBar-left{
	margin-right: 18px;
}
input.form-control {
	outline: none;
	box-shadow: none;
}

.form-control:focus {
	webkit-box-shadow: none;
	box-shadow: none;
}

.land{
	color: #666;
	display: inline-block;
	width: 50%;
	text-align: center;
	padding: 0 6px;
	cursor: pointer;
}
.land:hover{
	color: #0059e2;
}
ul.nav.nav-tabs {
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-bottom: 0;
	padding: 22px 0;
}

ul.nav.nav-tabs li {
	margin-bottom: 0;
	overflow: hidden;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a {
	display: inline-block;
	color: #666;
	font-size: 18px;
	white-space: inherit;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a:hover {
	cursor: pointer;
	text-align: none;
	color: #4b96cf;
	font-weight: bolder;
	background: none;
	border: none;
}

ul.nav.nav-tabs li a:visited,
a:active,
a:link {
	cursor: pointer;
	text-align: none;
	background: none;
}

.banner {
	width: 100%;
	height: 18rem;
	padding:1rem 0;
	background: #fff;
	background: url(../img/bannerFind.png) no-repeat;
	background-size: cover;
}
.infopname1{
	width: 100%;
	display: flex;
	height: 3rem;
	line-height: 3rem;
	font-size: 14px;
	color: #666666;
	justify-content: space-between;
}
.lastname{
	margin-bottom: 20px;
}
.infopname1 .sitename{
	
	
	
}

.rightman{
	float: right;
	font-size: 14px;
}

.webLink {
	width: 80%;
	padding: 25px 0;
	margin: 0 auto;
	display: flex;
	justify-content: space-between;
	background: #f5f6f6;
}
h5 {
	width: 100%;
	margin: 0;
	text-align: center;
	padding: 0;
	font-size: 24px;
	font-weight: bold;
	height: 4rem;
	line-height: 4rem;
	color:#4b96cf;
}
.processReg {
	width: 100%;
	margin: 0;
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 0;
	font-size: 30px;
	font-weight: bold;
	height: 8rem;
	
	color:#999999;
	background: #F2DEDE;
}
.processReg span {
	display: inline-block;
}
.ringnum{
	width: 50px;
	height: 50px;
	text-align: center;
	line-height: 50px;
	border:1px solid #cccc;
	border-radius: 50%;
}
.activeRing{
	background: #4b96cf;
	color:#fff;
	border: 1px solid #4b96cf;
}
.lineSpan{
	width: 13.3rem;
	border: 1px solid #cccccc;
	margin: 0 20px;
}
.yuqbottom {
	width: 68%;
	margin: 40px auto;
	min-height:65%;
	background: #fff;
	display: flex;
	align-items: center;
	justify-content: center;
}
.asp{
	width: 48rem;
    height: 35rem;
    background: #FF6501;
}
.yuqbottom:after{
    content: "";
    height: 0;
    clear: both;
    overflow: hidden;
    display: block;
    visibility: hidden;
}
.tail{
	margin-top: 2rem;
}
.webLinkWork {
	width: 100%;
	background: #fff;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 14px;
	color: #666666;
	padding: 40px 0;
}
.webLinkWork span{
	padding: 0 10px;
}
.webLinkWork a {
	cursor: pointer;
	text-align: none;
	font-size: 14px;
	color: #666666;
}
.webLinkWork a:hover{
	color: #4b96cf;
}


.banquan {
	width: 100%;
	padding: 15px 0;
	text-align: center;
	color: #313131;
	background: #eaeaea;
	font-size: 12px;
}

.btn{
	padding: 5px 20px;
	color: #fff;
	background:#4b96cf;
	font-size: 14px;
	
}