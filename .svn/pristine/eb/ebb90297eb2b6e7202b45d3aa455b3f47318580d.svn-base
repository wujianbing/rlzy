 .select-container {
            position: relative;
            flex: 1;
           
        }

        .select-container input {
            width: 80%;
            height: 3rem;

            outline: none;
            line-height: 24px;

            text-indent: 5px;
        }

        .select-container span {
            /* 瀹氫綅鑷� input 灏鹃儴 */
            position: absolute;
            top: 10px;
            left: 65%;

            /* 灞忚斀鐐瑰嚮浜嬩欢 */
            pointer-events: none;
        }

        .select-container ul {
            /* 瀹氫綅鑷� input 涓嬮潰 */
            position: absolute;
            top: 3rem;
            left: 0px;
            padding: 0;
            /* 鏄剧ず鍦� input 涓婇潰 */
            z-index: 1;
            width: 80%;
            /* 榛樿闅愯棌 */
            display: none;
            list-style: none;
            /* 杈规 */
            border: 1px solid #a9a9a9;;
        }

        .select-container li a {
            /* 浣胯儗鏅壊鍗犳弧涓�琛� */
            display: inline-block;
            width: 100%;

            color: #000000;
            text-indent: 5px;

            /* 榛樿鑳屾櫙鑹� */
            background: #fff;

            /* 鍘绘帀涓嬪垝绾� */
            text-decoration: none;
        }

        .select-container li a:hover {
            color: #fff;
            background: #1F92E4;
        }
