	background: #f5f6f6;
}
h5 {
	width: 100%;
	margin: 0;
	padding: 0;
	font-size: 16px;
	font-weight: bold;
	padding: 30px 0 10px;
	color:#4b96cf;
}

.yuqbottom {
	width: 68%;
	margin: 2rem auto 0;
	
	
	background: #f5f6f6;
}
.yuqbottom:after{
    content: "";
    height: 0;
    clear: both;
    overflow: hidden;
    display: block;
    visibility: hidden;
}
.yuleft{
	width: 70%;
	/*height: 60rem;*/
	float: left;
	background: #fff;
}
.yuright{
	width:28.5%;
	
