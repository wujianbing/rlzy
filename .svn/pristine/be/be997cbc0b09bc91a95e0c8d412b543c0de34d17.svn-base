html,
body {
	width: 100%;
	font-size: 62.5%;
	background: #f5f6f6;
}



* {
	padding: 0;
	margin: 0;
}

.titleTop {
	width: 100%;
	background: #fff;
}

.rowTop {
	width: 69%;
	background: #fff;
	margin: 0 auto;
	color: #666;
	white-space: nowrap;
	font-size: 16px;
}

.rowBar {
	width: 100%;
	padding: 0 10%;
	height: 60px;
	margin: 0;
	color: #666;
	/*box-shadow: 0px 23px 20px -20px #ddd;*/
	background: #fff;
}

.humanbox {
	width: 100%;
	padding: 0;
	margin: 0;
}

.title {
	height: 80px;
	color: #666;
}

.title h3 {
	font-weight: bolder;
	color: #666;
	font-size: 2.8rem;
}

.title h6 {
	color: #666;
}

.searchBar {
	height: 8rem;
	display: flex;
	align-items: center;
}

.input-group {
	border: none;
	background: #fff;
	padding: 0;
	margin: 0;
}

.input-group .searchBarLeft {
	border: none;
	font-size: 1.5rem;
	background: #f5f6f6;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
	outline: none;
}

.input-group .searchBarRight {
	padding: 0;
	margin: 0;
}

input.form-control {
	outline: none;
	box-shadow: none;
}

.form-control:focus {
	webkit-box-shadow: none;
	box-shadow: none;
}

.input-group .searchBarRight button.btn.btn-default {
	top: 0;
	border: none;
	padding: 7px 12px;
	background: #f5f6f6;
	border-top-right-radius: 15px;
	border-bottom-right-radius: 15px;
	outline: none;
	z-index: 0;
	margin-left: 0px;
}

button.btn.btn-default.glyphicon.glyphicon-search {
	color: #358acb;
	font-weight: bolder;
}

.login {
	color: #666;
	display: inline-block;
	width: 50%;
	text-align: right;
	padding-right: 25%;
}

.registered {
	color: #666;
	display: inline-block;
	width: 50%;
}

ul.nav.nav-tabs {
	width: 100%;
	height: 6rem;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-bottom: 0;
}

ul.nav.nav-tabs li {
	margin-bottom: 0;
	overflow: hidden;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a {
	display: inline-block;
	color: #666;
	font-size: 18px;
	white-space: inherit;
	padding: 0;
	margin: 0;
}

ul.nav.nav-tabs li a:hover {
	cursor: pointer;
	text-align: none;
	color: #4b96cf;
	font-weight: bolder;
	background: none;
	border: none;
}

ul.nav.nav-tabs li a:visited,
a:active,
a:link {
	cursor: pointer;
	text-align: none;
	background: none;
}

.banner {
	width: 100%;
	height: 18rem;
	padding:1rem 0;
	background: #fff;
	background: url(../img/bannerFind.png) no-repeat;
	background-size: cover;
}
.bannerdel{
	width: 67%;
    height: 16rem;
    padding: 2rem 0;
    margin: 0 auto;
}
.renshi{
	font-weight: bolder;
}
.time{
	font-size: 16px;
}
.findman{
	font-size: 18px;
	height: 4rem;
	line-height: 4rem;
	color: #fff;
}
.sq{
	padding: 0px 20px;
	font-size: 14px;
	background: #fff;
	height: 3rem;
    line-height: 3rem;
	color: #4b96cf;
	border-radius: 8px;
}
.infoname{
	width: 100%;
	padding: 0 10%;
	margin: 0 auto;
	border-bottom: 1px dashed #ccc;
}
.infopname1{
	width: 100%;
	display: flex;
	height: 3rem;
	line-height: 3rem;
	font-size: 14px;
	color: #666666;
}
.lastname{
	margin-bottom: 20px;
}
.infopname1 .sitename{
	
	display: inline-block;
	flex: 1;
}
.infopname1 .sitename:nth-of-type(2n){
	
	text-align: center;
}
.infopname1 .sitename:nth-of-type(3n){
	
