function MyFind(){
	
}
MyFind.prototype = {
	init: function(){
		myFind.bind();
	},
	bind: function(){
		//表格点击
		layui.use('table');
	},
	tab1Accept: function(res){
		console.log(res);
		// layer.alert('编辑行：<br>'+ JSON.stringify(res));
		//询问框
		layer.confirm('是否确定接受？', {
		  btn: ['确定','取消'] //按钮
		}, function(){
			$.get(url+"/user/accept?id="+res.relationId,function(data){
				  if("1" == data){
					  layer.msg('接受成功', {icon: 1});
				  }
			  });
		});
	},
	tab1Decline: function(res){
		layer.confirm('是否确定拒绝？', {
		  btn: ['确定','取消'] //按钮
		}, function(){
			$.get(url+"/user/refuse?id="+res.relationId,function(data){
				  if("1" == data){
					  layer.msg('拒绝成功', {icon: 1});
				  }
			  });
		});
	},
	tab2Decline: function(res){
		layer.confirm('是否取消收藏？', {
		  btn: ['确定','取消'] //按钮
		}, function(){
			$.get(url+"/user/cancel?id="+res.relationId,function(data){
				  if("1" == data){
					  layer.msg('取消成功', {icon: 1});
				  }
			  });
		});
	}
}
let myFind = new MyFind();
myFind.init();



//iframe自适应
function reinitIframe(){
	var iframe = document.getElementById("iframe");
	try{
	var bHeight = iframe.contentWindow.document.body.scrollHeight;
	var dHeight = iframe.contentWindow.document.documentElement.scrollHeight;
	var height = Math.max(bHeight, dHeight);
	$('#iframe').css({'height':height});
	console.log(height);
	}catch (ex){}
}
// iframe切换
$(function(){
	$('#navbar li').on('click',function(){
		//console.log($(this).attr('data-id'));
		let id = $(this).attr('data-id');
		let dom = $('#iframe');
		if(id == 1){
			dom.attr('src','pages/myResume.html');
			dom.css({'height':'1186px'});
		}else if(id == 2){
			dom.attr('src','pages/myJobwant.html');
			dom.css({'height':'600px'});
		}else if(id == 3){
			dom.attr('src','pages/myCollect.html');
			dom.css({'height':'600px'});
		}else if(id == 4){
			dom.attr('src',url+'/user/manage?userId='+userId);
			dom.css({'height':'600px'});
